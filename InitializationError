//This example shows an Initializing error

public class Scope{
    public static void main(String[] args){
        int x;
        if(x==10)
        {
        int y=20;
        System.out.println("x and y"+x+""+y);
        x= y*2;
        }
        //y=100;
        
        System.out.println("x is "+x);
    }
    
}
