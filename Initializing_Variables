//The correct form of initializing variables
//into a for cycle

public class Class {
    public static void main(String args[]) {
      int x;
      for(x=0;x<3;x++)
      {
          int y=-1;
          
          System.out.println("Y is "+y);
          y=100;
          System.out.println("Y is nos "+y);
      }

      
    }
}
