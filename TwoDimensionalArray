//This is an example of Two dimentional Arrays
public class TwoDArray{
 public static void main(String[] args){
     int TD[][] = new int[4][5];
     int i,j,k=0;
     for(i=0;i<4;i++)
     for(j=0;j<5;j++){
     TD[i][j] = k;
     
     k++;
     }
     
     for(i=0;i<4;i++)
     {
     
     for(j=0;j<5;j++)
     {
         System.out.print(TD[i][j] + " ");
     }
     
     System.out.println();
         
     }
     
 }   
    
}
